{
  "name": "openeyes/openeyes",
  "description": "Electronic Patient Record system for use in Ophthalmology clinics",
  "minimum-stability": "dev",
  "require": {
    "openeyes/module-installer": "*",
    "yiisoft/yii": "1.1.14",
    "openeyes/oph-ci-examination": "dev-develop",
    "openeyes/oph-co-correspondence": "1.10",
    "openeyes/oph-ci-phasing": "dev-master",
    "openeyes/oph-co-therapyapplication": "dev-master",
    "openeyes/oph-dr-prescription": "dev-develop",
    "openeyes/oph-ou-anaestheticsatisfactionaudit": "dev-master",
    "openeyes/oph-tr-consent": "1.10",
    "openeyes/oph-tr-operationnote": "dev-develop",
    "openeyes/oph-tr-operationbooking": "dev-develop",
    "openeyes/oph-tr-intravitrealinjection": "dev-develop",
    "openeyes/oph-tr-laser": "dev-master",
    "openeyes/patient-ticketing": "1.10",
    "openeyes/oph-in-visualfields": "dev-master",
    "openeyes/oph-in-biometry": "dev-develop",
    "openeyes/eyedraw": "dev-develop"
  },
  "require-dev": {
    "behat/behat": "~2",
    "behat/mink": "~1",
    "behat/yii-extension": "~1",
    "behat/mink-extension": "~1",
    "behat/mink-goutte-driver": "~1",
    "behat/mink-selenium2-driver": "~1",
    "sensiolabs/behat-page-object-extension": "1.0.1",
    "phpunit/phpunit": "3.7.*",
    "phpunit/phpunit-selenium": "1.3.*",
    "phpunit/dbunit": "1.2.*",
    "phpunit/php-invoker": "1.1.*",
    "phpunit/phpunit-story": "*",
    "phploc/phploc": "*",
    "pdepend/pdepend": "2.0.4",
    "phpmd/phpmd": "@stable",
    "squizlabs/php_codesniffer": "2.*",
    "sebastian/phpcpd": "*",
    "phpdocumentor/phpdocumentor": "2.*"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "classmap": ["features/bootstrap/Pages/OpenEyesPage.php"]
  }
}
